<html>
	<style>
		body{
				text-align:center;
			}
		.btn{
				width:200px;
  				padding:5px;
			}
		.bg-custom{
					background-color:#F2D7D5
		}
		.table{
					background:DarkSeaGreen
		}
	</style>
	<head>
  		<meta charset="utf-8">
  		<meta name="viewport" content="width=device-width, initial-scale=1">
  		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
	</head>
	<body class="bg-custom">
	<h1>
		View Survey Report
	</h1>
		<br>
	<h4>
		Survey Name: <%= data[0].SURVEY_NAME %>
	</h4>
		<br>
	<div class="container"style="width:700px">
	<table class="table table-bordered table-hover">
		<thead style="background:IndianRed">
		<tr><th> QUESTION</th>
			<th> RATING</th>
			</tr></thead>
		<% data.forEach(function(survey){ %>
		<tr><td><%= survey.QUESTION %></td>
			<td><%= survey.RATING %></td>
		</tr>
		<% }) %>
	</table>
	</div>
	<br>
	<form method="post" name="home" action="home">
		<button name="home" type="submit" class="btn btn-success">
			HOME
		</button>
	</form>
	</body>
</html>